class Solution {
    public int[] twoSum(int[] nums, int target) {
        int en=nums.length;
        for(int i=0;i<en-1;i++){
            for(int j=i+1;j<en;j++){
                if(nums[i]+nums[j]==target){
                    return new int[]{i,j};
                }
            }
            
        }
        return new int[]{ };
    }
}
